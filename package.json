{
	"name": "@ky-is/vue-separator",
	"version": "0.1.0",
	"description": "A renderless component that intersperses elements with a separator.",
	"main": "src/index.js",
	"author": "Kyle Coburn",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ky-is/vue-separator.git"
	},
	"keywords": [
		"vue",
		"markdown",
		"md"
	],
	"bugs": {
		"url": "https://github.com/ky-is/vue-separator/issues"
	},
	"homepage": "https://github.com/ky-is/vue-separator#readme",
	"scripts": {
		"test": "jest"
	},
	"devDependencies": {
		"@vue/test-utils": "^1.0.0-beta.20",
		"babel-jest": "^23.4.0",
		"babel-preset-env": "^1.7.0",
		"jest": "^23.4.0",
		"vue": "^2.5.16",
		"vue-jest": "^2.6.0",
		"vue-template-compiler": "^2.5.16"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"modules": false
				}
			]
		],
		"env": {
			"test": {
				"presets": [
					[
						"env",
						{
							"targets": {
								"node": "current"
							}
						}
					]
				]
			}
		}
	},
	"jest": {
		"testURL": "http://localhost",
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.vue$": "<rootDir>/node_modules/vue-jest"
		},
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		}
	}
}
